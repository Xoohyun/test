/***** Grid *****/
.Grid
{
	-nexa-border 		: -nexa-define(--Grid-Border);
	background 		: -nexa-define(--Grid-Background);
}
.Grid .head
{
	background 		: #f3f4f6;
}
.Grid .head .row .cell
{
	-nexa-border 		: 1px solid #e1e2e7;
	-nexa-text-align 	: center;
	color 			: #222222;
	font 			: bold 13px "Malgun Gothic";
}
.Grid .body .row .cell
{
	-nexa-border 		: 1px solid #eeeeee;
	-nexa-padding 		: 4px 4px 4px 4px;
}
.Grid .body .row .cell[status=mouseover]
{
	background 		: #f9f9f9;
}
.Grid .body .row .cell[userstatus=selected]
{
	background 		: #f0f9fd;
}
.Grid .summary
{
	background 		: #b2b9c6;
}
.Grid .summary .row .cell
{
	-nexa-border 		: 1px solid #d2d2d2; /*#d2d6de;*/
	-nexa-padding 		: 4px 10px 4px 10px;
	color 			: #ffffff;
	font 			: bold 14px "Malgun Gothic";
}
/* cellcalendar (Same as CalendarControl) */
.GridCellControl .cellcalendar
{
	font : 12px "Malgun Gothic";
}
/*cellcontrol Start*/
.GridCellControl .cellbutton
{
