<?xml version="1.0" encoding="utf-8"?>
<XCSS version="1.1">
  <Predefines>
    <Group id="MAIN COLORS">
      <Define id="Main-Primary-Color" value="#2A5AEE"/>
      <Define id="Main-Background" value="#ffffff"/>
    </Group>
    <Group id="MAIN BORDERS">
      <Define id="Main-Border" value="1px solid #dddddd"/>
      <Define id="Main-Border Radius" value="5px"/>
    </Group>
    <Group id="MAIN TEXT">
      <Define id="Main-Font" value="Malgun Gothic"/>
      <Define id="Main-Word Wrap" value="char"/>
    </Group>
    <Group id="Form">
