/***************************************************************************/

/***************************************************************************/
/*******************	BASE COMPONENT START	****************************/
/***************************************************************************/
/***** Button *****/
.Button
{
	-nexa-border 		: -nexa-define(--Button-Border);
	-nexa-padding 		: -nexa-define(--Button-Padding);
	background 		: -nexa-define(--Button-Background);
	border-radius 		: -nexa-define(--Main-Border Radius);
	cursor 			: pointer;
	color 			: -nexa-define(--Button-Color);
	letter-spacing 		: -1px;
}
.Button[status=mouseover],.Button[userstatus=pushed],
.Button[userstatus=selected]
{
	-nexa-border 		: 1px solid #313340;
}
.Button[status=disabled]
{
	opacity 		: 0.5;
}

