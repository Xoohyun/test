    <Group id="TextArea">
      <Define id="TextArea-Background" value="#ffffff"/>
      <Define id="TextArea-Border" value="1px solid #dddddd"/>
      <Define id="TextArea-Padding" value="8px 8px 0px 8px"/>
    </Group>
