    <Group id="MaskEdit">
      <Define id="MaskEdit-Background" value="#ffffff"/>
      <Define id="MaskEdit-Border" value="1px solid #dddddd"/>
      <Define id="MaskEdit-Padding" value="0px 8px 0px 0px"/>
