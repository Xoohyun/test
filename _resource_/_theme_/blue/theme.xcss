/****** Form *****/
.Form
{
	-nexa-border 		: -nexa-define(--Form-Border);
	background 		: -nexa-define(--Form-Background);
	color 			: -nexa-define(--Form-Color);
	font 			: 13px -nexa-define(--Main-Font);
}
.Form[userstatus=contents]
{
	background 		: transparent;
}
/***************************************************************************/
